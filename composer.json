{
	"name" : "wwesantos/arpabet-to-ipa",
	"description" : "Convert Arpabet to IPA. Arpabet is the set of phonemes used by the CMU Pronouncing Dictionary. IPA is the International Phonetic Alphabet",
	"keywords" : [
		"arpabet",
		"ipa",
		"dictionary",
		"phoneme",
		"pronunciation"
	],
	"homepage" : "http://github.com/wwesantos/arpabet-to-ipa",
	"license" : "MIT",
	"authors" : [{
			"name" : "Waldeilson Eder dos Santos",
			"homepage" : "http://www.linkedin.com/in/waldeilson/en"
		}
	],
	"require" : {
		"php" : ">=5.3.0"
	},
	"autoload" : {
		"psr-4" : {
			"ArpabetToIPA\\" : "/src"
		},
		"psr-0" : {
			"ArpabetToIPA\\" : "/src"
		}
	},
	"minimum-stability" : "dev",
	"require-dev" : {
		"php" : ">=5.3.0",
		"phpunit/phpunit" : "4.7.4",
		"squizlabs/php_codesniffer": "*",
   		"wimg/php-compatibility": "dev-master",
   		"simplyadmire/composer-plugins" : "@dev"
	}
}